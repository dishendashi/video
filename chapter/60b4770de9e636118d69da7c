var floweringId;
RunOnReady(function() {
    $('#loginform').on('login', function (event, usr) {
        $('#LoginRegiterModal').modal('hide');
        if (homagingId) {
            Homage(homagingId);
            delete homagingId;
        }
        if (floweringId) {
            Flower(floweringId);
            delete floweringId;
        }
        event.stopImmediatePropagation();
    });

    $.get('/counter/homage/60b44258e9e636118d69da03', function(result) {
        if (result.success) {
            $('#homageNo').html(result.homage);
            $('#flowerNo').html(result.flower);
        }
    })
});

// var Homaged = false;
function Homage(id) {
    //var already_ = function () {
    //    bootstrap_alert.show('<span class="glyphicon glyphicon-info-sign"/>&nbsp;您已经顶礼过！', 'warning', 6000);
    //};
    //if (!Homaged) {
        $.post('/activity/homage/'+ id, {type:'Video'},function (data) {
            if (data.success) {
                //Homaged = true;
                var ctl = $('#homageNo');
                if (ctl.html())
                    ctl.html((ctl.html()|0) + 1);
                else
                    ctl.html(1);
                //bootstrap_alert.show('<span class="glyphicon glyphicon-heart"/>&nbsp;顶礼成功', 'success', 4000);
            }
            else {
                if (data.reason == -5) {
                    bootstrap_alert.show('<span class="glyphicon glyphicon-user"/>&nbsp;尚未登录，请先登录', 'warning', 6000);
                    $('#LoginRegiterModal').modal('show');
                    homagingId = id;
                }
                else if (data.reason == -4) {
                    already_();
                }
            }
        });
    //}
    //else {
    //    already_();
    //}
}

//var Flowered = false;
function Flower(id) {
    var already_ = function () {
        bootstrap_alert.show('<span class="glyphicon glyphicon-info-sign"/>&nbsp;您已经献过花', 'warning', 6000);
    };
    //if (!Flowered) {
        $.post('/activity/flower/'+ id, {type:'Video'}, function (data) {
            if (data.success) {
                Flowered = true;
                var ctl = $('#flowerNo');
                if (ctl.html())
                    ctl.html((ctl.html() | 0) + 1);
                else
                    ctl.html(1);
                //bootstrap_alert.show('<span class="glyphicon glyphicon-heart"/>&nbsp;供花成功', 'success', 4000);
            }
            else {
                if (data.reason == -5) {
                    bootstrap_alert.show('<span class="glyphicon glyphicon-user"/>&nbsp; 尚未登录，请先登录', 'warning', 6000);
                    $('#LoginRegiterModal').modal('show');
                    floweringId = id;
                }
                else if (data.reason == -4) {
                    already_();
                }
            }
        });
    //- }
    //- else {
    //-     already_();
    //- }
    
}

